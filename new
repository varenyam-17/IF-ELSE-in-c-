#include<iostream>
using namespace std;
int main()
{
    int num1,num2,num3,num4;
   cout<<"Enter a number"<<endl;
   cin>>num1;
   cout<<"Enter a number"<<endl;
   cin>>num2;
   cout<<"Enter a number"<<endl;
   cin>>num3;
   cout<<"Enter a number"<<endl;
   cin>>num4;
   if(num1>num2)
   {
       if (num1>num3)
       {
           if(num1>num4)
           {
               cout<<"Greatest "<<num1<<endl;
           }
       }
   }
  else if(num2>num1)
   {
       if (num2>num3)
       {
           if(num2>num4)
           {
               cout<<"Greatest "<<num2<<endl;
           }
       }
   }
  else if(num3>num1)
   {
       if (num3>num2)
       {
           if(num3>num4)
           {
               cout<<"Greatest "<<num3<<endl;
           }
       }
   }
 else
   {
        cout<<"Greatest"<<num4<<endl;
       
   }
} 
