#include<iostream>
using namespace std;
int main()
{
    int ch;
    cout<<"1.Monday"<<endl<<"2.Teusday"<<endl<<"3.Wednesday"<<endl<<"4.Thursday"<<endl<<"5.Friday"<<endl<<"6.Saturday"<<endl<<"7.Sunday"<<endl;
    cout<<"Enter Your Choice~>"<<endl;
    cin>>ch;
    switch(ch)
    {
    case 1:
    cout<<"Monday"<<endl;
    break;
    case 2:
    cout<<"Teusday"<<endl;
     break;
    case 3:
    cout<<"Wednesday"<<endl;
     break;
    case 4:
    cout<<"Thursday"<<endl;
     break;
    case 5:
    cout<<"Friday"<<endl;
     break;
    case 6:
    cout<<"Saturday"<<endl;
     break;
    case 7:
    cout<<"Sunday"<<endl;
    }
}    
    
