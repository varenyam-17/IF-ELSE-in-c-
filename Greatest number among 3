#include<iostream>
using namespace std;
int main()
{
    int num1,num2,num3;
   cout<<"Enter a number"<<endl;
   cin>>num1;
   cout<<"Enter a number"<<endl;
   cin>>num2;
   cout<<"Enter a number"<<endl;
   cin>>num3;
   if(num1>num2&&num1>num3)
   {
       cout<<"Grestest "<<num1<<endl;
   }
   else if(num2>num1&&num2>num3)
   {
       cout<<"Grestest "<<num2<<endl;
   }
   else
   {
        cout<<"Grestest "<<num3<<endl;
   }
}  
